---
layout: nil
---

<?xml version="1.0" encoding="UTF-8" ?>
<feed xmlns="http://www.w3.org/2005/Atom">
	<title>Jing's Mind</title>
	<link href="http://www.funnyguo.me/" />
	<link rel="self" type="application/atom+xml" href="/atom.xml" />
	<id>http://www.funnyguo.me/</id>

	<author>
		<name>Jing Guo</name>
		<email>dev.guoj@gmail.com</email>
	</author>

	{% for post in site.posts %}
	<entry>
    <id>http://www.funnyguo.me/{{ post.id }}</id>
    <link type="text/html" rel="alternate" href="http://www.funnyguo.me/{{ post.url }}"/>
    <title>{{ post.title | xml_escape }}</title>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
        <name>Jing Guo</name>
        <uri>http://www.funnyguo.me/</uri>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
 
</feed>